{
    "title": "Update",

    "content": [
        {
            "type": "button",
            "label": "Update Now",

            "actions": [
                "self.update()"
            ]
        },
        {
            "type": "button",
            "label": "Patch Notes",

            "actions": [
                "self.renderMessage('patch_notes')"
            ]
        },
        {
            "type": "button",
            "label": "Back",

            "actions": [
                "self.switchPage(self.last_page)"
            ]
        }
    ]
}